ror) => {\r\n    //     console.log(error);\r\n    //   });\r\n\r\n    try {\r\n      // 호출 시작 => 로딩시작\r\n      this.setState({ loading: true });\r\n      const response = await axios.post('https://api.marktube.tv/v1/me', {\r\n        email,\r\n        password,\r\n      });\r\n      // sleep\r\n      await sleep(1000);\r\n\r\n      this.setState({ loading: false });\r\n      // 호출 완료 => 로딩끝\r\n      console.log(response.data.token);\r\n\r\n      // 토큰을 브라우저 어딘가에 저장한다.\r\n      localStorage.setItem('token', response.data.token);\r\n\r\n      // 페이지를 이동한다.\r\n      this.props.history.push('/');\r\n    } catch (error) {\r\n      this.setState({ loading: false });\r\n      //  호출 완료 => 로딩끝\r\n      console.log(error);\r\n    }\r\n  };\r\n\r\n  change = (e) => {\r\n    // console.log('change', e.target.value);\r\n    this.setState({ email: e.target.value });\r\n  };\r\n}\r\n\r\nexport default withRouter(Signin);\r\n","C:\\Users\\Windows\\Desktop\\react-tutorial\\my_book_list\\src\\hocs\\withToken.js",[],"C:\\Users\\Windows\\Desktop\\react-tutorial\\my_book_list\\src\\components\\BookList.jsx",[],"C:\\Users\\Windows\\Desktop\\react-tutorial\\my_book_list\\src\\utils.js",[],"C:\\Users\\Windows\\Desktop\\react-tutorial\\my_book_list\\src\\components\\BookItem.jsx",[],["80","81"],"C:\\Users\\Windows\\Desktop\\react-tutorial\\my_book_list\\src\\redux\\store.js",[],"C:\\Users\\Windows\\Desktop\\react-tutorial\\my_book_list\\src\\redux\\reducer.js",[],"C:\\Users\\Windows\\Desktop\\react-tutorial\\my_book_list\\src\\redux\\books.js",[],"C:\\Users\\Windows\\Desktop\\react-tutorial\\my_book_list\\src\\redux\\actions.js",[],"C:\\Users\\Windows\\Desktop\\react-tutorial\\my_book_list\\src\\containers\\BookListContainer.jsx",[],"C:\\Users\\Windows\\Desktop\\react-tutorial\\my_book_list\\src\\services\\BookService.js",[],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":18,"column":13,"nodeType":"88","messageId":"89",